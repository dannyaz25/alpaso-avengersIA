{
  "name": "alpaso-avengersIA-test-automation",
  "version": "1.0.0",
  "description": "Automated testing suite for Alpaso AvengersIA frontend and backend",
  "type": "module",
  "scripts": {
    "test": "npm run test:unit && npm run test:api",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:api": "jest --testPathPattern=tests/api",
    "test:mock": "node scripts/mock-test-runner.js",
    "test:functional": "node scripts/functional-test-runner.js",
    "test:all-working": "npm run test:unit && npm run test:api && npm run test:mock",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:api && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "setup": "npm run setup:frontend && npm run setup:backend",
    "setup:frontend": "cd ../frontend && npm install && npm run dev &",
    "setup:backend": "cd ../backend && npm install && npm run dev &",
    "kill:servers": "pkill -f 'vite' && pkill -f 'nodemon'",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test:assistants": "node scripts/assistant-test-runner.js",
    "test:voice": "node scripts/voice-test-runner.js",
    "test:automation": "node scripts/automation-test-runner.js"
  },
  "keywords": [
    "testing",
    "automation",
    "playwright",
    "jest",
    "e2e",
    "api-testing",
    "alpaso",
    "avengersIA",
    "voice-testing",
    "AI-assistants"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "socket.io-client": "^4.7.5",
    "web-audio-api": "^0.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.0",
    "@vitest/coverage-v8": "^0.34.6",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "@vitejs/plugin-react": "^4.1.0",
    "jsdom": "^23.0.1"
  }
}

